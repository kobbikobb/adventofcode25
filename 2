from _utils.text_utils import read_input_data
from day01.part1 import get_hits_part_1
from day01.part2 import get_hits_part_2


def main():
    """Run day"""
    data = read_input_data("input.txt")

    hits_1 = get_hits_part_1(data)
    print(f"hits 1 = {hits_1}")

    hits_2 = get_hits_part_2(data)
    print(f"hits 2 = {hits_2}")


if __name__ == "__main__":
    # 1018 Correct
    # 5815 Correct
    main()
